pipeline {
    agent any

    stages {
        stage('Hello Friends :') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Development') {
            steps {
                echo 'This is development stage'
            }
        }
        stage('Build') {
            steps {
                echo 'This is building stage'
            }
        }
        stage('Deploy') {
            steps {
                echo 'This is deployment stage'
            }
        }
        stage('Test') {
            steps {
                echo 'This is testing stage'
            }
        }
        stage('Release') {
            steps {
                echo 'This is realising stage'
            }
        }
    }
}
